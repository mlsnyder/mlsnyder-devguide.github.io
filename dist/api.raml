#%RAML 1.0
title: AvaTax API
version: v2
baseUri: https://sandbox-rest.avatax.com/
documentation:
- title: AvaTax API
  content: REST interface to Avalara's enterprise tax service, AvaTax.
securitySchemes:
  auth:
    type: Basic Authentication
types:
  AddressValidationInfo:
    displayName: AddressValidationInfo
    description: TextCase info for input address
    type: object
    properties:
      textCase:
        required: false
        displayName: textCase
        description: Specify the text case for the validated address result.  If not specified, will return uppercase.
        type: string
        enum:
        - Upper
        - Mixed
      line1:
        required: false
        displayName: line1
        description: Line1
        type: string
      line2:
        required: false
        displayName: line2
        description: Line2
        type: string
      line3:
        required: false
        displayName: line3
        description: Line3
        type: string
      city:
        required: false
        displayName: city
        description: City
        type: string
      region:
        required: false
        displayName: region
        description: State / Province / Region
        type: string
      country:
        required: false
        displayName: country
        description: Two character ISO 3166 Country Code
        type: string
      postalCode:
        required: false
        displayName: postalCode
        description: Postal Code / Zip Code
        type: string
      latitude:
        required: false
        displayName: latitude
        description: Geospatial latitude measurement
        type: number
        format: double
      longitude:
        required: false
        displayName: longitude
        description: Geospatial longitude measurement
        type: number
        format: double
  AddressResolutionModel:
    displayName: AddressResolutionModel
    description: Address Resolution Model
    type: object
    properties:
      address:
        required: false
        displayName: address
        description: The original address
        type: AddressInfo
      validatedAddresses:
        required: false
        displayName: validatedAddresses
        description: The validated address or addresses
        type: array
        items:
          type: ValidatedAddressInfo
      coordinates:
        required: false
        displayName: coordinates
        description: The geospatial coordinates of this address
        type: CoordinateInfo
      resolutionQuality:
        required: false
        displayName: resolutionQuality
        description: The resolution quality of the geospatial coordinates
        type: string
        enum:
        - NotCoded
        - External
        - CountryCentroid
        - RegionCentroid
        - PartialCentroid
        - PostalCentroidGood
        - PostalCentroidBetter
        - PostalCentroidBest
        - Intersection
        - Interpolated
        - Rooftop
        - Constant
      taxAuthorities:
        required: false
        displayName: taxAuthorities
        description: List of informational and warning messages regarding this address
        type: array
        items:
          type: TaxAuthorityInfo
      messages:
        required: false
        displayName: messages
        description: List of informational and warning messages regarding this address
        type: array
        items:
          type: AvaTaxMessage
  AddressInfo:
    displayName: AddressInfo
    description: Represents an address to resolve.
    type: object
    properties:
      line1:
        required: false
        displayName: line1
        description: Line1
        type: string
      line2:
        required: false
        displayName: line2
        description: Line2
        type: string
      line3:
        required: false
        displayName: line3
        description: Line3
        type: string
      city:
        required: false
        displayName: city
        description: City
        type: string
      region:
        required: false
        displayName: region
        description: State / Province / Region
        type: string
      country:
        required: false
        displayName: country
        description: Two character ISO 3166 Country Code
        type: string
      postalCode:
        required: false
        displayName: postalCode
        description: Postal Code / Zip Code
        type: string
      latitude:
        required: false
        displayName: latitude
        description: Geospatial latitude measurement
        type: number
        format: double
      longitude:
        required: false
        displayName: longitude
        description: Geospatial longitude measurement
        type: number
        format: double
  ValidatedAddressInfo:
    displayName: ValidatedAddressInfo
    description: Represents a validated address
    type: object
    properties:
      addressType:
        required: false
        displayName: addressType
        description: "Address type code. One of: \r\n* F - Firm or company address\r\n* G - General Delivery address\r\n* H - High-rise or business complex\r\n* P - PO Box address\r\n* R - Rural route address\r\n* S - Street or residential address"
        type: string
      line1:
        required: false
        displayName: line1
        description: Line1
        type: string
      line2:
        required: false
        displayName: line2
        description: Line2
        type: string
      line3:
        required: false
        displayName: line3
        description: Line3
        type: string
      city:
        required: false
        displayName: city
        description: City
        type: string
      region:
        required: false
        displayName: region
        description: State / Province / Region
        type: string
      country:
        required: false
        displayName: country
        description: Two character ISO 3166 Country Code
        type: string
      postalCode:
        required: false
        displayName: postalCode
        description: Postal Code / Zip Code
        type: string
      latitude:
        required: false
        displayName: latitude
        description: Geospatial latitude measurement
        type: number
        format: double
      longitude:
        required: false
        displayName: longitude
        description: Geospatial longitude measurement
        type: number
        format: double
  CoordinateInfo:
    displayName: CoordinateInfo
    description: Coordinate Info
    type: object
    properties:
      latitude:
        required: false
        displayName: latitude
        description: Latitude
        type: number
        format: double
      longitude:
        required: false
        displayName: longitude
        description: Longitude
        type: number
        format: double
  TaxAuthorityInfo:
    displayName: TaxAuthorityInfo
    description: Tax Authority Info
    type: object
    properties:
      jurisdictionName:
        required: true
        displayName: jurisdictionName
        description: Jurisdiction Name
        type: string
      avalaraId:
        required: false
        displayName: avalaraId
        description: Avalara Id
        type: string
      jurisdictionType:
        required: false
        displayName: jurisdictionType
        description: Jurisdiction Type
        type: string
        enum:
        - Country
        - Composite
        - State
        - County
        - City
        - Special
      signatureCode:
        required: false
        displayName: signatureCode
        description: Signature Code
        type: string
  AvaTaxMessage:
    displayName: AvaTaxMessage
    description: Informational or warning messages returned by AvaTax with a transaction
    type: object
    properties:
      summary:
        required: false
        displayName: summary
        description: A brief summary of what this message tells us
        type: string
      details:
        required: false
        displayName: details
        description: Detailed information that explains what the summary provided
        type: string
      refersTo:
        required: false
        displayName: refersTo
        description: Information about what object in your request this message refers to
        type: string
      severity:
        required: false
        displayName: severity
        description: A category that indicates how severely this message affects the results
        type: string
      source:
        required: false
        displayName: source
        description: The name of the code or service that generated this message
        type: string
  BatchModel:
    displayName: BatchModel
    description: Represents a batch of uploaded documents.
    type: object
    properties:
      name:
        required: true
        displayName: name
        description: The user-friendly readable name for this batch.
        type: string
      type:
        required: true
        displayName: type
        description: The type of this batch.
        type: string
        enum:
        - AvaCertUpdate
        - AvaCertUpdateAll
        - BatchMaintenance
        - CompanyLocationImport
        - DocumentImport
        - ExemptCertImport
        - ItemImport
        - SalesAuditExport
        - SstpTestDeckImport
        - TaxRuleImport
        - TransactionImport
        - UPCBulkImport
        - UPCValidationImport
      id:
        required: false
        displayName: id
        description: The unique ID number of this batch.
        type: integer
        format: int32
      accountId:
        required: false
        displayName: accountId
        description: The Account ID number of the account that owns this batch.
        type: integer
        format: int32
      companyId:
        required: false
        displayName: companyId
        description: The Company ID number of the company that owns this batch.
        type: integer
        format: int32
      status:
        required: false
        displayName: status
        description: This batch's current processing status
        type: string
        enum:
        - Waiting
        - SystemErrors
        - Cancelled
        - Completed
        - Creating
        - Deleted
        - Errors
        - Paused
        - Processing
      options:
        required: false
        displayName: options
        description: Any optional flags provided for this batch
        type: string
      batchAgent:
        required: false
        displayName: batchAgent
        description: The agent used to create this batch
        type: string
      startedDate:
        required: false
        displayName: startedDate
        description: The date/time when this batch started processing
        type: datetime
      recordCount:
        required: false
        displayName: recordCount
        description: The number of records in this batch; determined by the server
        type: integer
        format: int32
      currentRecord:
        required: false
        displayName: currentRecord
        description: The current record being processed
        type: integer
        format: int32
      completedDate:
        required: false
        displayName: completedDate
        description: The date/time when this batch was completely processed
        type: datetime
      createdDate:
        required: false
        displayName: createdDate
        description: The date when this record was created.
        type: datetime
      createdUserId:
        required: false
        displayName: createdUserId
        description: The User ID of the user who created this record.
        type: integer
        format: int32
      modifiedDate:
        required: false
        displayName: modifiedDate
        description: The date/time when this record was last modified.
        type: datetime
      modifiedUserId:
        required: false
        displayName: modifiedUserId
        description: The user ID of the user who last modified this record.
        type: integer
        format: int32
      files:
        required: false
        displayName: files
        description: The list of files contained in this batch.
        type: array
        items:
          type: BatchFileModel
  BatchFileModel:
    displayName: BatchFileModel
    description: Represents one file in a batch upload.
    type: object
    properties:
      name:
        required: true
        displayName: name
        description: Logical Name of file (e.g. "Input" or "Error").
        type: string
      content:
        required: true
        displayName: content
        description: Content of the batch file.
        type: string
      contentType:
        required: true
        displayName: contentType
        description: Content mime type (e.g. text/csv).  This is used for HTTP downloading.
        type: string
      fileExtension:
        required: true
        displayName: fileExtension
        description: File extension (e.g. CSV).
        type: string
      id:
        required: false
        displayName: id
        description: The unique ID number assigned to this batch file.
        type: integer
        format: int32
      batchId:
        required: false
        displayName: batchId
        description: The unique ID number of the batch that this file belongs to.
        type: integer
        format: int32
      contentLength:
        required: false
        displayName: contentLength
        description: Size of content, in bytes.
        type: integer
        format: int32
      errorCount:
        required: false
        displayName: errorCount
        description: Number of errors that occurred when processing this file.
        type: integer
        format: int32
  Fetchresult[batchmodel]:
    displayName: FetchResult[BatchModel]
    type: object
    properties:
      '@recordsetCount':
        required: false
        displayName: '@recordsetCount'
        type: integer
        format: int32
      value:
        required: false
        displayName: value
        type: array
        items:
          type: BatchModel
      '@nextLink':
        required: false
        displayName: '@nextLink'
        type: string
  CompanyModel:
    displayName: CompanyModel
    description: A company or business entity.
    type: object
    properties:
      id:
        required: true
        displayName: id
        description: The unique ID number of this company.
        type: integer
        format: int32
      accountId:
        required: true
        displayName: accountId
        description: The unique ID number of the account this company belongs to.
        type: integer
        format: int32
      companyCode:
        required: true
        displayName: companyCode
        description: A unique code that references this company within your account.
        type: string
      name:
        required: true
        displayName: name
        description: The name of this company, as shown to customers.
        type: string
      defaultCountry:
        required: true
        displayName: defaultCountry
        description: The two character ISO-3166 country code of the default country for this company.
        type: string
      parentCompanyId:
        required: false
        displayName: parentCompanyId
        description: If this company is fully owned by another company, this is the unique identity of the parent company.
        type: integer
        format: int32
      sstPid:
        required: false
        displayName: sstPid
        description: If this company files Streamlined Sales Tax, this is the PID of this company as defined by the Streamlined Sales Tax governing board.
        type: string
      isDefault:
        required: false
        displayName: isDefault
        description: This flag is true if this company is the default company for this account.  Only one company may be set as the default.
        type: boolean
      defaultLocationId:
        required: false
        displayName: defaultLocationId
        description: If set, this is the unique ID number of the default location for this company.
        type: integer
        format: int32
      isActive:
        required: false
        displayName: isActive
        description: This flag indicates whether tax activity can occur for this company.  Set this flag to true to permit the company to process transactions.
        type: boolean
      taxpayerIdNumber:
        required: false
        displayName: taxpayerIdNumber
        description: "For United States companies, this field contains your Taxpayer Identification Number.  \r\nThis is a nine digit number that is usually called an EIN for an Employer Identification Number if this company is a corporation, \r\nor SSN for a Social Security Number if this company is a person.\r\nThis value is required if you subscribe to Avalara Managed Returns or the SST Certified Service Provider services, \r\nbut it is optional if you do not subscribe to either of those services."
        type: string
      hasProfile:
        required: false
        displayName: hasProfile
        description: |
          Set this flag to true to give this company its own unique tax profile.

          If this flag is true, this company will have its own Nexus, TaxRule, TaxCode, and Item definitions.

          If this flag is false, this company will inherit all profile values from its parent.
        type: boolean
      isReportingEntity:
        required: false
        displayName: isReportingEntity
        description: |
          Set this flag to true if this company must file its own tax returns.

          For users who have Returns enabled, this flag turns on monthly Worksheet generation for the company.
        type: boolean
      sstEffectiveDate:
        required: false
        displayName: sstEffectiveDate
        description: If this company participates in Streamlined Sales Tax, this is the date when the company joined the SST program.
        type: datetime
      baseCurrencyCode:
        required: false
        displayName: baseCurrencyCode
        description: This is the three character ISO-4217 currency code of the default currency used by this company.
        type: string
      roundingLevelId:
        required: false
        displayName: roundingLevelId
        description: Indicates whether this company prefers to round amounts at the document level or line level.
        type: string
        enum:
        - Line
        - Document
      warningsEnabled:
        required: false
        displayName: warningsEnabled
        description: Set this value to true to receive warnings in API calls via SOAP.
        type: boolean
      isTest:
        required: false
        displayName: isTest
        description: |
          Set this flag to true to indicate that this company is a test company.

          If you have Returns enabled, Test companies will not file tax returns and can be used for validation purposes.
        type: boolean
      taxDependencyLevelId:
        required: false
        displayName: taxDependencyLevelId
        description: Used to apply tax detail dependency at a jurisdiction level.
        type: string
        enum:
        - Document
        - State
        - TaxRegion
        - Address
      inProgress:
        required: false
        displayName: inProgress
        description: |
          Set this value to true to indicate that you are still working to finish configuring this company.

          While this value is true, no tax reporting will occur and the company will not be usable for transactions.
        type: boolean
      businessIdentificationNo:
        required: false
        displayName: businessIdentificationNo
        description: Business Identification No
        type: string
      createdDate:
        required: false
        displayName: createdDate
        description: The date when this record was created.
        type: datetime
      createdUserId:
        required: false
        displayName: createdUserId
        description: The User ID of the user who created this record.
        type: integer
        format: int32
      modifiedDate:
        required: false
        displayName: modifiedDate
        description: The date/time when this record was last modified.
        type: datetime
      modifiedUserId:
        required: false
        displayName: modifiedUserId
        description: The user ID of the user who last modified this record.
        type: integer
        format: int32
      contacts:
        required: false
        displayName: contacts
        description: 'Optional: A list of contacts defined for this company.  To fetch this list, add the query string "?$include=Contacts" to your URL.'
        type: array
        items:
          type: ContactModel
      items:
        required: false
        displayName: items
        description: 'Optional: A list of items defined for this company.  To fetch this list, add the query string "?$include=Items" to your URL.'
        type: array
        items:
          type: ItemModel
      locations:
        required: false
        displayName: locations
        description: 'Optional: A list of locations defined for this company.  To fetch this list, add the query string "?$include=Locations" to your URL.'
        type: array
        items:
          type: LocationModel
      nexus:
        required: false
        displayName: nexus
        description: 'Optional: A list of nexus defined for this company.  To fetch this list, add the query string "?$include=Nexus" to your URL.'
        type: array
        items:
          type: NexusModel
      settings:
        required: false
        displayName: settings
        description: 'Optional: A list of settings defined for this company.  To fetch this list, add the query string "?$include=Settings" to your URL.'
        type: array
        items:
          type: SettingModel
      taxCodes:
        required: false
        displayName: taxCodes
        description: 'Optional: A list of tax codes defined for this company.  To fetch this list, add the query string "?$include=TaxCodes" to your URL.'
        type: array
        items:
          type: TaxCodeModel
      taxRules:
        required: false
        displayName: taxRules
        description: 'Optional: A list of tax rules defined for this company.  To fetch this list, add the query string "?$include=TaxRules" to your URL.'
        type: array
        items:
          type: TaxRuleModel
      upcs:
        required: false
        displayName: upcs
        description: 'Optional: A list of UPCs defined for this company.  To fetch this list, add the query string "?$include=UPCs" to your URL.'
        type: array
        items:
          type: UPCModel
  ContactModel:
    displayName: ContactModel
    description: A contact person for a company.
    type: object
    properties:
      id:
        required: true
        displayName: id
        description: The unique ID number of this contact.
        type: integer
        format: int32
      companyId:
        required: true
        displayName: companyId
        description: The unique ID number of the company to which this contact belongs.
        type: integer
        format: int32
      contactCode:
        required: true
        displayName: contactCode
        description: A unique code for this contact.
        type: string
      firstName:
        required: false
        displayName: firstName
        description: The first or given name of this contact.
        type: string
      middleName:
        required: false
        displayName: middleName
        description: The middle name of this contact.
        type: string
      lastName:
        required: false
        displayName: lastName
        description: The last or family name of this contact.
        type: string
      title:
        required: false
        displayName: title
        description: Professional title of this contact.
        type: string
      line1:
        required: false
        displayName: line1
        description: The first line of the postal mailing address of this contact.
        type: string
      line2:
        required: false
        displayName: line2
        description: The second line of the postal mailing address of this contact.
        type: string
      line3:
        required: false
        displayName: line3
        description: The third line of the postal mailing address of this contact.
        type: string
      city:
        required: false
        displayName: city
        description: The city of the postal mailing address of this contact.
        type: string
      region:
        required: false
        displayName: region
        description: The state, region, or province of the postal mailing address of this contact.
        type: string
      postalCode:
        required: false
        displayName: postalCode
        description: The postal code or zip code of the postal mailing address of this contact.
        type: string
      country:
        required: false
        displayName: country
        description: The ISO 3166 two-character country code of the postal mailing address of this contact.
        type: string
      email:
        required: false
        displayName: email
        description: The email address of this contact.
        type: string
      phone:
        required: false
        displayName: phone
        description: The main phone number for this contact.
        type: string
      mobile:
        required: false
        displayName: mobile
        description: The mobile phone number for this contact.
        type: string
      fax:
        required: false
        displayName: fax
        description: The facsimile phone number for this contact.
        type: string
      createdDate:
        required: false
        displayName: createdDate
        description: The date when this record was created.
        type: datetime
      createdUserId:
        required: false
        displayName: createdUserId
        description: The User ID of the user who created this record.
        type: integer
        format: int32
      modifiedDate:
        required: false
        displayName: modifiedDate
        description: The date/time when this record was last modified.
        type: datetime
      modifiedUserId:
        required: false
        displayName: modifiedUserId
        description: The user ID of the user who last modified this record.
        type: integer
        format: int32
  ItemModel:
    displayName: ItemModel
    description: Represents an item in your company's product catalog.
    type: object
    properties:
      id:
        required: true
        displayName: id
        description: The unique ID number of this item.
        type: integer
        format: int64
      companyId:
        required: true
        displayName: companyId
        description: The unique ID number of the company that owns this item.
        type: integer
        format: int32
      itemCode:
        required: true
        displayName: itemCode
        description: A unique code representing this item.
        type: string
      description:
        required: true
        displayName: description
        description: A friendly description of this item in your product catalog.
        type: string
      taxCodeId:
        required: false
        displayName: taxCodeId
        description: |
          The unique ID number of the tax code that is applied when selling this item.

          When creating or updating an item, you can either specify the Tax Code ID number or the Tax Code string; you do not need to specify both values.
        type: integer
        format: int32
      taxCode:
        required: false
        displayName: taxCode
        description: |
          The unique code string of the Tax Code that is applied when selling this item.

          When creating or updating an item, you can either specify the Tax Code ID number or the Tax Code string; you do not need to specify both values.
        type: string
      createdDate:
        required: false
        displayName: createdDate
        description: The date when this record was created.
        type: datetime
      createdUserId:
        required: false
        displayName: createdUserId
        description: The User ID of the user who created this record.
        type: integer
        format: int32
      modifiedDate:
        required: false
        displayName: modifiedDate
        description: The date/time when this record was last modified.
        type: datetime
      modifiedUserId:
        required: false
        displayName: modifiedUserId
        description: The user ID of the user who last modified this record.
        type: integer
        format: int32
  LocationModel:
    displayName: LocationModel
    description: |
      A location where this company does business.

      Some jurisdictions may require you to list all locations where your company does business.
    type: object
    properties:
      id:
        required: true
        displayName: id
        description: The unique ID number of this location.
        type: integer
        format: int32
      companyId:
        required: true
        displayName: companyId
        description: The unique ID number of the company that operates at this location.
        type: integer
        format: int32
      locationCode:
        required: true
        displayName: locationCode
        description: A code that identifies this location.  Must be unique within your company.
        type: string
      addressTypeId:
        required: true
        displayName: addressTypeId
        description: Indicates whether this location is a physical place of business or a temporary salesperson location.
        type: string
        enum:
        - Location
        - Salesperson
      addressCategoryId:
        required: true
        displayName: addressCategoryId
        description: Indicates the type of place of business represented by this location.
        type: string
        enum:
        - Storefront
        - MainOffice
        - Warehouse
        - Salesperson
        - Other
      line1:
        required: true
        displayName: line1
        description: The first line of the physical address of this location.
        type: string
      postalCode:
        required: true
        displayName: postalCode
        description: The postal code or zip code of the physical address of this location.
        type: string
      description:
        required: false
        displayName: description
        description: A friendly name for this location.
        type: string
      line2:
        required: false
        displayName: line2
        description: The second line of the physical address of this location.
        type: string
      line3:
        required: false
        displayName: line3
        description: The third line of the physical address of this location.
        type: string
      city:
        required: false
        displayName: city
        description: The city of the physical address of this location.
        type: string
      county:
        required: false
        displayName: county
        description: The county name of the physical address of this location.  Not required.
        type: string
      region:
        required: false
        displayName: region
        description: The state, region, or province of the physical address of this location.
        type: string
      country:
