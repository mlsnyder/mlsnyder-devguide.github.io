  TransactionLineModel:
    displayName: TransactionLineModel
    description: One line item on this transaction.
    type: object
    properties:
      id:
        required: false
        displayName: id
        description: The unique ID number of this transaction line item.
        type: integer
        format: int64
      transactionId:
        required: false
