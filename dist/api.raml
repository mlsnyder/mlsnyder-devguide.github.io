      line3:
        required: false
        displayName: line3
        description: Line3
        type: string
      city:
        required: false
        displayName: city
        description: City
        type: string
      region:
        required: false
        displayName: region
        description: State / Province / Region
        type: string
      country:
        required: false
        displayName: country
        description: Two character ISO 3166 Country Code
        type: string
      postalCode:
        required: false
        displayName: postalCode
        description: Postal Code / Zip Code
        type: string
      latitude:
        required: false
        displayName: latitude
        description: Geospatial latitude measurement
        type: number
        format: double
      longitude:
        required: false
        displayName: longitude
        description: Geospatial longitude measurement
        type: number
        format: double
  LineItemModel:
    displayName: LineItemModel
