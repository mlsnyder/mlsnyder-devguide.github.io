        type: number
        format: double
      amount:
        required: true
        displayName: amount
        description: Total amount for this line
        type: number
        format: double
      number:
        required: false
        displayName: number
        description: Line number within this document
        type: string
      taxCode:
        required: false
        displayName: taxCode
        description: Tax Code - System or Custom Tax Code.
        type: string
      itemCode:
        required: false
        displayName: itemCode
        description: Item Code (SKU)
        type: string
      description:
        required: false
        displayName: description
        description: Item description.  This is required for SST transactions if an unmapped ItemCode is used.
        type: string
  TaxOverrideModel:
    displayName: TaxOverrideModel
    description: Represents a tax override for a transaction
    type: object
    properties:
      type:
        required: false
        displayName: type
        description: Identifies the type of tax override
        type: string
        enum:
