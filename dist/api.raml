        - ReturnInvoice
        - InventoryTransferOrder
        - InventoryTransferInvoice
        - ReverseChargeOrder
        - ReverseChargeInvoice
        - Any
      code:
        required: false
        displayName: code
        description: |
          Transaction Code - the internal reference code used by the client application.  This is used for operations such as

          Get, Adjust, Settle, and Void.  If you leave the transaction code blank, a GUID will be assigned to each transaction.
        type: string
      companyCode:
        required: false
        displayName: companyCode
        description: |
          Company Code - Specify the code of the company creating this transaction here.  If you leave this value null,

          your account's default company will be used instead.
        type: string
      purchaseOrderNo:
        required: false
        displayName: purchaseOrderNo
        description: |
          Purchase Order Number for this document

          This is required for single use exemption certificates to match the order and invoice with the certificate.
        type: string
      addresses:
        required: false
        displayName: addresses
        description: Default addresses for all lines in this document
        type: AddressesModel
      lines:
        required: false
        displayName: lines
        description: Document line items list
        type: array
        items:
          type: LineItemModel
      commit:
        required: false
        displayName: commit
        description: "Causes the document to be committed if true.  This option is only applicable for invoice document \r\ntypes, not orders."
        type: boolean
      currencyCode:
        required: false
        displayName: currencyCode
        description: 3 character ISO 4217 currency code.
        type: string
      description:
        required: false
        displayName: description
        description: Description
        type: string
  AddressesModel:
